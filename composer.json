{
    "name": "mileschou/pherm",
    "description": "A command line terminal utility in PHP",
    "keywords": [
        "cli",
        "console",
        "terminal"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Miles Chou",
            "email": "jangconan@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-posix": "*",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "amphp/amp": "^2.4",
        "illuminate/container": "^7.19",
        "mockery/mockery": "^1.3",
        "phpstan/phpstan": "^0.12.32",
        "phpunit/phpunit": "^8.5",
        "psy/psysh": "^0.10",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\Pherm\\": "src/Pherm"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.2.5"
        },
        "preferred-install": "dist",
        "sort-packages": true
    }
}
